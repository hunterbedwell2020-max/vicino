{
  "name": "vicino-backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "sh -c 'set -a; [ -f .env ] && . ./.env; set +a; tsx watch src/server.ts'",
    "build": "tsc -p tsconfig.json",
    "start": "sh -c 'set -a; [ -f .env ] && . ./.env; set +a; node dist/server.js'"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "pg": "^8.13.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.7",
    "@types/pg": "^8.11.11",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
