{
  "expo": {
    "name": "Vicino",
    "slug": "vicino",
    "version": "0.1.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#5B2A86"
    },
    "ios": {
      "supportsTablet": false
    },
    "android": {},
    "web": {},
    "plugins": [
      "expo-asset",
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Vicino uses your location to show nearby profiles and meetups."
        }
      ]
    ]
  }
}
