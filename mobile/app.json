{
  "expo": {
    "name": "Vicino",
    "slug": "vicino",
    "version": "0.1.0",
    "scheme": "vicino",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#5B2A86"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.hunterbedwell.vicino",
      "infoPlist": {
        "NSCameraUsageDescription": "Vicino uses your camera for profile and identity verification photos.",
        "NSPhotoLibraryUsageDescription": "Vicino uses your photo library for profile and identity verification photos.",
        "NSUserTrackingUsageDescription": "Vicino does not use tracking across apps.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.hunterbedwell.vicino",
      "permissions": [
        "CAMERA",
        "READ_MEDIA_IMAGES",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "POST_NOTIFICATIONS",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {},
    "plugins": [
      "expo-asset",
      "expo-notifications",
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Vicino uses your location to show nearby profiles and meetups."
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "2b911e7f-63f7-4f37-90cd-eb0ac21bf353"
      }
    }
  }
}
